<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" editor-extension-mode="False">
    <Style src="project://database/Assets/UI/MainMenuButton.uss?fileID=7433441132597879392&amp;guid=c4099f03a414d28448584b771a575bf9&amp;type=3#MainMenuButton" />
    <ui:VisualElement name="menuBoard" style="align-items: flex-start; justify-content: space-between; height: 100%; flex-direction: row;">
