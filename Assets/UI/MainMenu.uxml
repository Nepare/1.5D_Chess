            <ui:Button text="Options" display-tooltip-when-elided="true" name="ButtonOptions" class="MainMenuButton" style="left: 12px; bottom: 12px; position: relative; -unity-text-align: middle-center; height: 10%; width: 85%; padding-bottom: 0; padding-top: 0;" />
            <ui:Button text="Quit" display-tooltip-when-elided="true" name="ButtonQuit" class="MainMenuButton" style="left: 12px; bottom: 12px; position: relative; height: 10%; width: 85%; padding-top: 0; padding-bottom: 0;" />
